{
  "caption": "Analytic",
  "name": "analytic",
  "description": "The Analytic object describes the technique used for the analysis and extraction of insights from event data or information, ultimately contributing to the creation of findings or conclusions.",
  "extends": "_entity",
  "attributes": {
    "category": {
      "description": "The analytic category.",
      "requirement": "optional"
    },
    "desc": {
      "description": "The analytic description.",
      "requirement": "optional"
    },
    "name": {
      "description": "The analytic name."
    },
    "type": {
      "description": "The analytic type.",
      "requirement": "optional"
    },
    "type_id": {
      "description": "The analytic type identifier.",
      "requirement": "required",
      "enum": {
        "0": {
          "caption": "Unknown"
        },
        "1": {
          "caption": "Rule"
        },
        "2": {
          "caption": "Behavioral"
        },
        "3": {
          "caption": "Statistical"
        },
        "4": {
          "caption": "Learning (ML/DL)"
        },
        "5": {
          "caption": "Policy"
        },
        "99": {
          "caption": "Other"
        }
      }
    },
    "uid": {
      "description": "The analytic unique identifier."
    },
    "creator": {
      "description": "The user that created the analytic.",
      "requirement": "optional"
    },
    "version": {
      "description": "The analytic version. For example: <code>1.1</code>.",
      "requirement": "optional"
    }
  }
}
